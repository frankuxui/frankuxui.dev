<div set:html={Astro.props.code} class="syntax-highlight">
  <style>
    .syntax-highlight {
      background-color: #f5f5f5;
      padding: 1em;
      border-radius: 8px;
      overflow-x: auto;
    }
    .syntax-highlight pre {
      margin: 0;
      font-family: "Courier New", Courier, monospace;
    }
    .syntax-highlight code {
      color: #333;
    }
  </style>
  <slot />
</div>
